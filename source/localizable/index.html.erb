---
pageable: true
per_page: 10
---

	<header class="intro">
		<div class="intro-body">
			<div id="google-container"></div>
			<div id="particles-js"></div>
			<div id="le-logo">
				<div>
					<div class="in">
						<h1>#YESWECROWD</h1>
					 	<div id="cbp-qtrotator" class="cbp-qtrotator">
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot1) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot2) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot3) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot4) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot5) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot6) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot7) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot8) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot9) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot10) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot11) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot12) %></p></blockquote></div>
					 		<div class="cbp-qtcontent"><blockquote><p><%= t(:quot13) %></p></blockquote></div>
						</div>
					</div>
				</div>
			</div>
			<a class="logo-smartangels" href="https://www.smartangels.fr/entreprises/numa" target="_blank"><%= image_tag 'logo_smartangels.png', :alt => "Logo SmartAngels" %></a>
		</div>
	</header>

	<div id="main" role="main" class="home clearfix">
		<div class="container">
			<div class="row">
				<div class="col-sm-8">
					<section id="campaign">
						<h2><%= t(:pageheading) %></h2>
						<p><em><%= t(:motmarie) %></em>
						</p>
						<strong class="sig"><%= image_tag 'marie.jpg', :alt => "Marie Vorgan Le Barzic" %> <strong>Marie Vorgan Le Barzic <em>CEO</em></strong></strong>
						<div class="smartangels">
							<h3><%= t(:operationheading) %></h3>
							<p><%= t(:uniquemodel) %> <a href="http://SmartAngels.fr">SmartAngels.fr</a></p>
							<p><strong><%= t(:campaignduration) %></strong></p>
						</div>
						<div class="action"> <a class="btn btn-blue" href="https://www.smartangels.fr/entreprises/numa"><%= t(:participate) %></a> </div>
					</section>

					<div class="le-video">
						<div class="fluid">
							<!-- <div class="mask" frameborder="0"><iframe src="https://www.youtube.com/embed/co7_qw9W87o" frameBorder="0" allowfullscreen></iframe></div> -->
						</div>
					</div>
					
					<section id="ambition">
						
						<h2><%= t(:headingambition) %></h2>
						
						<div class="row">
							
							<div class="col-sm-8">
								<%= image_tag 'map.png', :class => "img-responsive", :alt => "Expanssion Map" %>
							</div>

							<div class="col-sm-4">
								<h4><%= t(:firstambition) %></h4>
							</div>

						</div>

						<div class="row">
							<div class="col-sm-12">
								<h4><%= t(:secondambition) %></h4>
								<p><%= t(:secondambitiondetails) %></p>
							</div>
						</div>

						<h2><%= t(:numamodel) %></h2>
						<div class="row">
							<div class="col-sm-6">
								<p><%= t(:modeldetails) %></p>
							</div>
							<div class="col-sm-6">
								<%= image_tag 'diagram.png', :class => "img-responsive", :alt => "NUMA Expanssion Map" %>
							</div>
						</div>
						<p><strong class="sig"><%= image_tag 'lucas.jpg', :alt => "Lucas Francou" %> <strong>Lucas Francou <em><%= t(:lucasjobdesc) %></em></strong></strong></p>
						<div class="action"> <a class="btn btn-blue" href="https://www.smartangels.fr/entreprises/numa"><%= t(:participate) %></a> </div>
					</section>
					
					<section id="realisations">
						<h2><%= t(:headingproof)%></h2>
						<div class="posts">
							<% blog.articles.select do |article| %>
								<% if article.data.category == 'blogging' && article.data.lang == t(:lang) %>
									<article>
									  <h5><a href="<%= article.url %>"><%= article.title %></a></h5>
									  <%= article.summary(111, '') %>
									</article>
								<% end %>
							<% end %>
						</div>
					</section>
					
					<section id="meetups">
						<h3>Meetups</h3>
						<p> <%= t(:meetups) %> <a href="mailto:yeswecrowd@numa.paris">yeswecrowd@numa.paris</a></p>
						<p><a target="_blank" class="btn btn-black" href="https://www.numa.paris/Evenements/Num-Apero-7-Soiree-speciale-YesWeCrowd"><%= t(:register) %></a>
					</section>
			  </div>

			  <div class="col-sm-4">
			  	<div id="myAffix">
			  		<div id="timeline">
			  			<ul role="tablist">
			  				<li class="bref">
									<a href="https://www.smartangels.fr/entreprises/numa" target="_blank"><%= image_tag 'logo_smartangels.png', :alt => "Logo SmartAngels" %></a>
			  					<em><%= t(:campaigndiscover) %> Smartangels.fr</em>
			  				</li>
			  				<li><a class="btn btn-blue" href="https://www.smartangels.fr/entreprises/numa"><%= t(:participate) %></a></li>
			  				<li class="last"><a target="_blank" href="http://yeswecrowd.co/images/kit_presse.pdf"><%= t(:presskit) %></a></li>
			  	    </ul>
			    	</div>
						
			  	</div>
			  </div>

			</div>
		</div> <!-- .container -->

	<div class="now">

		<div class="container">
		<h4 class="text-center"><%=t(:fameheading)%></h4>
		
		<ul class="la-presse">
			<li><a target="_blank" title="Numa, fidèle soutien des start-up parisiennes, change de dimension" target="_blank" href="http://www.lefigaro.fr/secteur/high-tech/2015/04/22/01007-20150422ARTFIG00194-numa-fidele-soutien-des-start-up-parisiennes-change-de-dimension.php"><%= image_tag 'le_figaro.png', :alt => "Le Figaro", :class => "figaro" %></a></li>
			<li><a target="_blank" title="L'accélérateur de startups Numa met le turbo dans les pays émergents" target="_blank" href="http://www.latribune.fr/technos-medias/start-up/l-accelerateur-de-startups-numa-met-le-turbo-dans-les-pays-emergents-470827.html"><%= image_tag 'la_tribune.png', :alt => "La Tribune", :class => "tribune" %></a></li>
			<li><a target="_blank" title="Numa ouvre son capital et espère lever jusqu'à 1 million d'euros" href="http://www.journaldunet.com/web-tech/start-up/numa-levee-fonds-internationalisation-0415.shtml"><%= image_tag 'jdn.png', :alt => "JDN", :class => "jdn" %></a></li>
			<li><a target="_blank" title="NUMA : le crowdequity, levier d'un changement de modèle" href="http://www.atelier.net/trends/articles/numa-crowdequity-levier-un-changement-de-modele_435087"><%= image_tag 'atelier.png', :alt => "L’ATELIER", :class => "atelier" %></a></li>
			<li><a target="_blank" title="NUMA OUVRE SON CAPITAL" href="http://www.ladn.eu/actualites/marque,numa-ouvre-capital,30,26107.html?utm_campaign=newsletter-s17-23_04_2015&utm_source=docnews-newsletter&utm_medium=email&utm_content=numa-ouvre-capital"><%= image_tag 'l_adn.png', :alt => "l' ADN", :class => "ladn" %></a></li>
			<li><a target="_blank" title="Numa cherche des fonds pour conquérir le monde" href="https://www.lejournaldugrandparis.fr/numa-cherche-des-fonds-pour-conquerir-le-monde/"><%= image_tag 'jgp.png', :alt => "Le Journal du Grand Paris", :class => "jgp" %></a></li>
			<li><a target="_blank" title="NUMA se donne 4 ans pour accélérer 700 startups dans le monde entier" href="http://www.maddyness.com/accompagnement/accelerateurs/2015/04/23/yeswecrowd-numa-se-donne-4-ans-pour-accelerer-700-startups-dans-le-monde-entier/"><%= image_tag 'maddyness.png', :alt => "Maddyness", :class => "maddyness" %></a></li>
			<li><a target="_blank" title="Numa change de modèle et de dimension" href="http://www.usine-digitale.fr/article/numa-change-de-modele-et-de-dimension.N326225"><%= image_tag 'lusine_digitale.png', :alt => "L'usine Digitale", :class => "usine" %></a></li>
			<li><a target="_blank" title="A Paris, Numa ouvre son capital et s’exporte hors des frontières" href="http://www.lesechos.fr/pme-regions/actualite-pme/02123799893-a-paris-numa-ouvre-son-capital-et-sexporte-hors-des-frontieres-1113586.php"><%= image_tag 'les_echos.png', :alt => "Les Echos", :class => "echos" %></a></li>
			<li><a target="_blank" title="Le numa espère lever un million d’euros" href="http://www.strategies.fr/actualites/marques/1013225W/le-numa-espere-lever-un-million-d-euros.html"><%= image_tag 'strategies.png', :alt => "Strategies", :class => "strategies" %></a></li>
			<li><a target="_blank" title="#YesWeCrowd NUMA lance sa campagne de crowdfunding" href="http://www.digitalbusinessnews.com/yeswecrowd-numa-lance-sa-campagne-de-crowdfunding"><%= image_tag 'digital_business_news.png', :alt => "DIGITAL BUSINESS NEWS", :class => "digital" %></a></li>
		</ul>
	</div>

	<%= partial "footer" %>
	
	</div>

</div> <!-- end MAIN -->

<%= javascript_include_tag  'jquery.min', 'jquery.easing.min', 'particles', 'bootstrap.min', 'app', 'quote'%>

<script> $( function() { $( '#cbp-qtrotator' ).cbpQTRotator(); } ); </script>